package util;

public class Velocity {
	
	private double x,y;
	
	public Velocity(double x, double y) {
		this.x = x;
		this.y = y;
	}
	public void normalize() {
		double tempX = x/(x+y);
		y = y/(x+y);
		x = tempX;
	}
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
}
